<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bypass.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/bpicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.bypass.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.bypass.app.IndividualBarActivity"
            android:label="Bar Info"
            android:parentActivityName="com.example.bypass.app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name="com.example.bypass.app.TransactionActivity"
            android:label="Bypass the Line"
            android:parentActivityName=".IndividualBarActivity">
        <!-- android:parentActivityName="com.example.bypass.app.IndividualBarActivity" >
         <meta-data
             android:name="android.support.PARENT_ACTIVITY"
             android:value="IndividualBarActivity" /> -->
        </activity>
    </application>

</manifest>
